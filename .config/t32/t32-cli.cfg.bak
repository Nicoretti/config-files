[t32]
   # REQUIRED
   sys  = /installation/dir/of/t32
   # OPTIONAL
   tmp  = /tmp/dir/of/t32
   # OPTIONAL
   help = /help/dir/of/t32 # default value = SYS_PATH/pdf

